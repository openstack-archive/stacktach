server {
	listen   80;
	server_name localhost;

    location / { try_files $uri @stacktach; }

    location @stacktach {
        include uwsgi_params;
        uwsgi_pass unix:/var/tmp/stacktach.sock;
    }

    location /static {
	    add_header Cache-Control public;
	    alias /usr/share/stacktach/static/;
    }
}
